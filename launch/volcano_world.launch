<?xml version="1.0" encoding="UTF-8"?>
<launch>
<!-- Launch an empty world simulation -->
  <include file="$(find gazebo_ros)/launch/volcano_world.launch">
    <arg name="world_name" value="$(find torino_quadcopter)/worlds/volcano.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
</launch>